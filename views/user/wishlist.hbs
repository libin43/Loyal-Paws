	<div class="cart_container">
		<div class="container">
			<div class="row">
				<div class="col">
					<div class="cart_title">your wishlist</div>
				</div>
			</div>
			<div class="row">
				<div class="col">
					<div class="cart_bar d-flex flex-row align-items-center justify-content-start">
						<div class="cart_bar_title_name">Product</div>
						<div class="cart_bar_title_content ml-auto">
							<div class="cart_bar_title_content_inner d-flex flex-row align-items-center justify-content-between">
								<div class="cart_bar_title_price">Price</div>
								
								
								<div class="cart_bar_title_button"></div>
								<div class="cart_bar_title_content_inner d-flex flex-row align-items-center justify-content-end">
									<div class="cart_bar_title_price">Action</div>
									
								</div>
							</div>
								
						</div>
					</div>
				</div>
			</div>
			<div class="row">
				<div class="col">
					<div class="cart_products">
						{{#each wishProducts}}
                        <ul>
							
							<li class=" cart_product d-flex flex-md-row flex-column align-items-md-center align-items-start justify-content-start">
								<!-- Product Image -->
								<div class="cart_product_image"><img style="width:100px ;height:100px ;" src="../product-images/{{this.wishItems.imageMany.[0]}}" alt=""></div>
								<!-- Product Name -->
								<div class="cart_product_name"><a href="product.html">{{this.wishItems.product}}</a></div>
								<div class="cart_product_info ml-auto">
									<div class="cart_product_info_inner d-flex flex-row align-items-center justify-content-md-end justify-content-start">
								    
									<div class="cart_product_info_inner d-flex flex-row align-items-center justify-content-md-end justify-content-start">

										<!-- Product Price -->
										

										<div class="cart_product_price">Rs. {{this.wishItems.price}}</div>
										
										
									
							

										<div class="cart_product_info_inner d-flex flex-row align-items-center justify-content-md-end justify-content-between">
											{{!-- <div class="cart_product_price">Rs. {{this.wishItems.price}}</div> --}}
										</div>
									
											<div class="mx-auto my-auto pr-2"><button onclick="add2Cart('{{this.wishItems._id}}','{{this._id}}')"
													style="background-color: #937c6f; color: white; height: 3em;" class="btn cart_button">
													ADD TO CART</button>
											</div>
										
										<!-- Product Cart Trash Button -->
										
										<div class="cart_product_button">
											<button class="cart_product_remove" onclick="deleteWishProduct('{{this._id}}','{{this.wishItems._id}}')" ><img src="images/trash.png" alt=""></button>
										</div>
										
                                      </div>
									 
									
									  
									</div>
									
									
								</div>
								
							</li>
							
						</ul>
                        {{/each}}

						{{#if noWishProducts}}
						<h2 class="mt-3">Your Wishlist is empty</h2>
						{{/if}}
