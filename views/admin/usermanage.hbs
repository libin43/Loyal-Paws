



  <main id="main" class="main">

    <div class="pagetitle">
      <h1> USER MANAGEMENT</h1>
      <nav>
        <ol class="breadcrumb">
          <li class="breadcrumb-item"><a href="index.html">Home</a></li>
          <li class="breadcrumb-item">Tables</li>
          <li class="breadcrumb-item active">General</li>
        </ol>
      </nav>
    </div><!-- End Page Title -->

    <section class="section">
      <div class="row">
        <div class="col-12">

          <div class="card ">
            <div class="card-body">
              <h5 class="card-title">Table</h5>

              <!-- Default Table -->
              <table class="table">
                <thead>
                  <tr>
                    <th scope="col">NO</th>
                    <th scope="col">NAME</th>
                    <th scope="col">EMAIL</th>
                    <th scope="col">MOBILE</th>
                    
                    <th scope="col">STATUS</th>
                    <th scope="col">ACTION</th>
                  </tr>
                </thead>
                <tbody>
                  {{#each users}}
                  <tr>
                    <th scope="row">{{@index}}</th>
                    <td>{{this.name}}</td>
                    <td>{{this.email}}</td>
                    <td>{{this.phone}}</td>
                 
                    <td><button style="background-color: rgb(255, 59, 59);">Block</button> <button style="background-color: rgb(66, 174, 66);">Unblock</button></td>
                    <td>delete</td>
                  </tr>
                  {{/each}}

                </tbody>
              </table>
              <!-- End Default Table Example -->
            </div>
          </div>

        


        </div>
      </div>
    </section>

  </main><!-- End #main -->




